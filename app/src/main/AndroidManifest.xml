<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.areaandvolume">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AreaAndVolume">
        <activity android:name=".volume.Cube"></activity>
        <activity android:name=".volume.Cone" />
        <activity android:name=".volume.Cylinder" />
        <activity android:name=".volume.Sphere" />
        <activity android:name=".area.Circle" />
        <activity android:name=".area.Triangle" />
        <activity android:name=".area.Rectangle" />
        <activity android:name=".area.Square" />
        <activity android:name=".PerformedActions" />
        <activity android:name=".Volumes" />
        <activity android:name=".Areas" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>