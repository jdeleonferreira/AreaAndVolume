<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.areaandvolume">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AreaAndVolume">
        <activity android:name=".volume.CubeActivity"></activity>
        <activity android:name=".volume.ConeActivity" />
        <activity android:name=".volume.CylinderActivity" />
        <activity android:name=".volume.SphereActivity" />
        <activity android:name=".area.CircleActivity" />
        <activity android:name=".area.TriangleActivity" />
        <activity android:name=".area.RectangleActivity" />
        <activity android:name=".area.SquareActivity" />
        <activity android:name=".PerformedActionsActivity" />
        <activity android:name=".VolumesActivity" />
        <activity android:name=".AreasActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>